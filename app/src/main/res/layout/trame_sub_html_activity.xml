    <include layout="@layout/main_title_fitment"></include>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <WebView
            android:id="@id/trame_sub_webview_fist"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="40dp">

        </WebView>

        <include
            layout="@layout/main_teme_foot"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:layout_gravity="bottom">

        </include>
    </FrameLayout>
</LinearLayout>